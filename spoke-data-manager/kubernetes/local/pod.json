{
    "apiVersion": "v1",
    "kind": "Pod",
    "metadata": {
        "name": "data-manager"
    },
    "spec": {
        "containers": [
            {
                "name": "data-manager",
                "image": "dictybase/migration-data-manager",
                "volumeMounts": [
                    {
                        "mountPath": "/data",
                        "name": "data-manager-folder"
                    }
                ],
                "env": [
                    {
                        "name": "BIOPORTAL_API_KEY",
                        "value": "92f0dc96-86ce-4098-ada5-695356448ca6"
                    }
                ],
                "command": [
                    "app",
                    "--bp",
                    "so",
                    "--bp",
                    "oborel",
                    "--bp",
                    "eco",
                    "--bp",
                    "cl",
                    "--gh"
                ]
            }
        ],
        "volumes": [
            {
                "name": "data-manager-folder",
                "emptyDir": {}
            }
        ],
        "restartPolicy": "Never"
    }
}
