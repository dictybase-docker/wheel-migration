{
    "kind": "PersistentVolume",
    "spec": {
        "accessModes": [
            "ReadWriteOnce",
            "ReadOnlyMany"
        ],
        "capacity": {
            "storage": "50Gi"
        },
        "gcePersistentDisk": {
            "pdName": "database-disk",
            "fsType": "xfs"
        }
    },
    "apiVersion": "v1",
    "metadata": {
        "name": "pv1",
        "labels": {
            "storage": "database"
        }
    }
}
