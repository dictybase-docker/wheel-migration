{
    "kind": "PersistentVolumeClaim",
    "apiVersion": "v1",
    "metadata": {
        "name": "postgres-claim1",
        "labels": {
            "storage": "postgres",
            "data": "complete",
            "type": "local"
        }
    },
    "spec": {
        "accessModes": [
            "ReadWriteOnce"
        ],
        "resources": {
            "requests": {
                "storage": "20G"
            }
        }
    }
}
